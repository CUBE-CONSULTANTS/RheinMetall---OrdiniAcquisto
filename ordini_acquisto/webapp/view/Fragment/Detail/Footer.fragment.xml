<c:FragmentDefinition
xmlns:m="sap.m"
	xmlns:c="sap.ui.core"
>
<m:OverflowToolbar> 
				<m:Button
					type="Emphasized"
					text="{i18n>Indietro}"
					icon="sap-icon://undo"
					press="onNav"
				/>
          
            <m:ToolbarSpacer/>

             <m:Button       
                icon="{= ${userModel>/tipoUtente} === 'Interno' ? 'sap-icon://cancel' : 'sap-icon://request'}"
                text="{= ${userModel>/tipoUtente} === 'Interno' ? ${i18n>NonAccettare} : ${i18n>RichiediMod}}"
                type="Emphasized"
                
            />
            <m:ToolbarSpacer/>   
            <m:Button
            icon="sap-icon://accept"
            text="{= ${userModel>/tipoUtente} === 'Interno' ? ${i18n>Conferma} : ${i18n>Save}}"
            type="Emphasized"
            
        /> 

	</m:OverflowToolbar>
</c:FragmentDefinition>