<mvc:View
    controllerName="ordiniacquisto.ordiniacquisto.controller.Home"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    height="100%"
    xmlns:core="sap.ui.core"
    xmlns="sap.m"
    xmlns:layout="sap.ui.layout"
>
    <Page
        class="sapUiResponsivePadding--header sapUiResponsivePadding--footer sapUiResponsivePadding--content"
        floatingFooter="true" backgroundDesign="Transparent" enableScrolling="true" showFooter="true" 
    >
        <customHeader>
          <core:Fragment
          fragmentName="ordiniacquisto.ordiniacquisto.view.Fragment.Header"
          />
        </customHeader>
        <!-- <subHeader>
          <core:Fragment
          fragmentName="ordiniacquisto.ordiniacquisto.view.Fragment.SubHeader"
          />
        </subHeader> -->
        <content>
            <core:Fragment
                fragmentName="ordiniacquisto.ordiniacquisto.view.Fragment.filterBar"
            />
            <core:Fragment
                fragmentName="ordiniacquisto.ordiniacquisto.view.Fragment.treeTable"
            />
        </content>
        <footer>
            <core:Fragment
                fragmentName="ordiniacquisto.ordiniacquisto.view.Fragment.Footer"
            />
        </footer>
    </Page>
</mvc:View>
